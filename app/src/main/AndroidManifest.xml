<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.proteam.bai_11_service">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".broadcasts.BroadcastActivity" />
        <activity android:name=".services.bounds.MainBindActivity" />
        <activity android:name=".services.un_bounds.MainStartActivity" />
        <activity android:name=".services.intent_service.ExampleIntentActivity" />

        <service android:name=".services.intent_service.ExampleIntentService" />
    </application>

</manifest>